TABLE:MATCH

	create table MATCH
	(
	Matchid varchar(10)primary key,
	Team1 varchar(10)references TEAM,
	Team2 varchar(10)references TEAM,
	Date1 date,
	Time varchar(10),
	Stadiumid varchar(10)references STADIUM
	);
	insert into MATCH(MATCHID,TEAM1,TEAM2,DATE1,TIME,STADIUMID)
	values('M001','T001','T002','03-APR-2018','9:30','S001');

	insert into MATCH(MATCHID,TEAM1,TEAM2,DATE1,TIME,STADIUMID)
	values('M002','T003','T004','05-APR-2018','2:30','S002');

	insert into MATCH(MATCHID,TEAM1,TEAM2,DATE1,TIME,STADIUMID)
	values('M003','T004','T005','07-APR-2018','2:30','S002');

	insert into MATCH(MATCHID,TEAM1,TEAM2,DATE1,TIME,STADIUMID)
	values('M004','T001','T003','08-APR-2018','9:30','S003');

	insert into MATCH(MATCHID,TEAM1,TEAM2,DATE1,TIME,STADIUMID)
	values('M005','T002','T005','09-APR-2018','9:30','S004');

	insert into MATCH(MATCHID,TEAM1,TEAM2,DATE1,TIME,STADIUMID)
	values('M006','T003','T005','11-APR-2018','2:30','S005');

	insert into MATCH(MATCHID,TEAM1,TEAM2,DATE1,TIME,STADIUMID)
	values('M007','T001','T004','12-APR-2018','9:30','S002');

	insert into MATCH(MATCHID,TEAM1,TEAM2,DATE1,TIME,STADIUMID)
	values('M008','T004','T002','14-APR-2018','2:30','S002');

