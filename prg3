TABLE:RESULT

	create table RESULT 
	(
	Matchid varchar(10)references MATCH,
	Teamwon varchar(10)references TEAM,
	Manofthematch varchar(10)references PLAYER
	);

	insert into RESULT(MATCHID,TEAMWON,MANOFTHEMATCH)
	values('M001','T001','P001');

	insert into RESULT(MATCHID,TEAMWON,MANOFTHEMATCH)
