TABLE:TEAM

	create table TEAM
	(
	Teamid varchar(10)primary key,
	Teamname varchar(10),
	City varchar(10),
	Coach varchar(10)
	);

	insert into TEAM(teamid,teamname,city, coach)
	values('T001','RCB','Banglore','Dravid',);

	insert into TEAM(teamid,teamname,city, coach)
	values('T002','CSK','Chennai','vettroi',);

	insert into TEAM(teamid,teamname,city, coach)
	values('T003','KKR','kalkata','RaviSankar',);

	insert into TEAM(teamid,teamname,city, coach)
	values('T004','DD','Delhi','Gavashar',);

	insert into TEAM(teamid,teamname,city, coach)
	values('T005','MI','Mumbai','Jayavardan',);

----------------------------------------------------------------------
TEAMID	TEAMNAME	CITY	COACH
T001	RCB		Bangalore	Dravid
T002	CSK		Chennai	Vettroi
T003	KKR		Kalkata	RaviSankar
T004	DD		Delhi	Gavashar
T005	MI		Mumbai	Jayavardan
----------------------------------------------------------------------

	TO ALTER THE TABEL;

	alter table TEAM add(Captain varchar(10)references PLAYER);

	table alter

	TO INSERT VALUES TO THE CAPTAIN ATTRIBUTE;

	update TEAM SET Captain= 'P001' where Teamid= 'T001';
	update TEAM SET Captain= 'P002' where Teamid= 'T002';
	update TEAM SET Captain= 'P003' where Teamid= 'T003';
	update TEAM SET Captain= 'P004' where Teamid= 'T004';
	update TEAM SET Captain= 'P005' where Teamid= 'T005';

-------------------------------------------------------------------------------------------------
TEAMID	TEAMNAME	CITY	COACH		CAPTAIN
T001	RCB		Bangalore	Dravid		P001
T002	CSK		Chennai	Vettroi		P002
T003	KKR		Kalkata	RaviSankar	P003
T004	DD		Delhi	Gavashar		P004
T005	MI		Mumbai	Jayavardan	P005
