TABLE:PLAYER

	create table PLAYER
	(	playerid varchar(10)primary key,
		Name varchar(10),
		Age number(2),
		Teamid varchar(10)references TEAM
	);


	insert into PLAYER(playerid,NAME,Age,Teamid)
	values('P001','Kohil',34,'T001');

	insert into PLAYER(playerid,NAME,Age,Teamid)
